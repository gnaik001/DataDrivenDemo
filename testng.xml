  </test> <!-- DashBoardTest -->
  
  <test thread-count="5" name="LoginTest">
    <classes>
      <class name="com.testCases.LoginTest"/>
    </classes>
  </test> <!-- LoginTest -->
  
 
</suite> <!-- Suite -->
